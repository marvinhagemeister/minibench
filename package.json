{
  "name": "minibench",
  "version": "0.0.2",
  "description": "Minimal benchmark library for nodejs",
  "main": "dist/index.js",
  "author": "Marvin Hagemeister <marvin@marvinhagemeister.de>",
  "license": "MIT",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:marvinhagemeister/minibench.git"
  },
  "scripts": {
    "cleanup": "rimraf dist/",
    "lint": "tslint 'src/**/*.ts'",
    "watch": "tsc -w",
    "build": "npm run cleanup && tsc",
    "test": "mocha -r ts-node/register -t 10000 'src/**/__tests__/*.spec.ts'",
    "test-watch": "npm t -- -w -R min",
    "prepublishOnly": "npm run lint && npm t && npm run build"
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "performance-now": "^2.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.2",
    "chai": "^4.1.2",
    "mocha": "^5.0.5",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-sevenval": "^1.0.1",
    "typescript": "^2.8.1"
  }
}
